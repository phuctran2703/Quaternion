<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <!-- Tell the system this app requires OpenGL ES 2.0. -->
    <uses-feature android:glEsVersion="0x00020000" android:required="true" />
    <supports-gl-texture android:name="GL_OES_compressed_ETC1_RGB8_texture" />
    <supports-gl-texture android:name="GL_OES_compressed_paletted_texture" />


    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.OpenGL"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.CarRotationActivity">
        </activity>
        <activity
            android:name=".activity.CarRotationResultActivity">
        </activity>
        <activity
            android:name=".activity.CarRotationAtTimeActivity">
        </activity>
        <activity
            android:name=".activity.CarRotationAtTimeResultActivity">
        </activity>
        <activity
            android:name=".activity.CarRotationIntervalActivity">
        </activity>
        <activity
            android:name=".activity.CarRotationIntervalResultActivity">
        </activity>
    </application>

</manifest>